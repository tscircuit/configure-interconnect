    "circuit-to-svg": "^0.0.271",
    "connectivity-map": "^1.0.0",
    "react": "^19.2.0",
    "react-cosmos": "^7.0.0",
    "react-cosmos-plugin-vite": "^7.0.0",
    "react-dom": "^19.2.0",
    "schematic-symbols": "^0.0.202",
